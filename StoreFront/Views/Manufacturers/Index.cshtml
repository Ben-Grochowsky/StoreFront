@model IEnumerable<StoreFront.DATA.EF.Models.Manufacturer>

@{
    ViewData["Title"] = "Manufacturers";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CompanyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address1)
